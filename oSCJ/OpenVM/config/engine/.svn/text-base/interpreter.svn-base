## -engine=interpreter selects the bytecode interpreter

# compile the interpreter's C code during image build.
s3.services.bootimage.ImageObserver \
	s3.core.services.interpreter.ImageCompiler(OVMMakefile.in, \
						   gdbinit.in)

# Garbage collectors must traverse interpreter stacks
#ovm.core.services.memory.LocalReferenceIterator \
#	s3.core.services.interpreter.InterpreterLocalReferenceIterator
# FIXME: need to make interpreter's support for accurate stack walking work!
ovm.core.services.memory.LocalReferenceIterator \
	s3.services.memory.conservative.ConservativeLocalReferenceIterator

ovm.core.execution.Activation$Factory \
	s3.core.services.interpreter.InterpreterActivation$Factory

