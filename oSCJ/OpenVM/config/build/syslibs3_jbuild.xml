<project name = "syslib"    default = "compile"    basedir = "../../src/syslib/s3">

<path  id = "syslib.classpath">
     <pathelement location = "." />
     <pathelement location = "../.." /></path>

<target name = "jdkVersion">
    <condition property = "syslib.isJDK140">
      <equals arg1 = "${java.version}" arg2="1.4.0"/></condition></target>

<target name = "compile" unless = "syslib.isJDK140">
     <javac 
          srcdir="." 
          debug="on"
	  bootclasspathref="syslib.classpath" 
          target="1.1"/></target>

<target name = "clean">
    <delete>
      <fileset dir = "." 
	includes   = "**/*.*.*~,**/*.class" /></delete></target>
 
</project>
