# @configure_generated@
# locate important files in an uninstalled OVM tree

## -image-argument
##    If ovm executables must be passed in the path to a binary
##    heap image file, this flag's default value will be "img", which 
##    works when ovm is run from the same directory where it is built.  If 
##    the contents of this file are embedded in the ovm executable, the 
##    default value of -image-argument will be the empty string.
##
##    This option is intened for use with ovm-config, and is not used
##    by gen-ovm itself.  See the regression test makefile for sample 
##    usage.
-image-argument "@IMAGE_ARGUMENT@"

-ovm-home	"@ABS_TOP_BUILDDIR@"
-ovm.jar	"@ABS_TOP_BUILDDIR@/src/ovm.jar"
-ovm_rt.jar	"@ABS_TOP_BUILDDIR@/src/syslib/s3/ovm_rt.jar"
-ovm_platform.jar "@ABS_TOP_BUILDDIR@/src/syslib/user/ovm_platform/ovm_platform.jar"
-ovm_rt_user.jar "@ABS_TOP_BUILDDIR@/src/syslib/user/ovm_rt_user.jar"
-ovm_rt_user_scj.jar "@ABS_TOP_BUILDDIR@/src/syslib/user/ovm_scj/ovm_rt_user_scj.jar"
-ovm_rt_user_realtime.jar "@ABS_TOP_BUILDDIR@/src/syslib/user/ovm_realtime/ovm_rt_user_realtime.jar"
-ovm_apps.jar	"@ABS_TOP_BUILDDIR@/src/apps/ovm_apps.jar"

-native-dir	"@ABS_TOP_SRCDIR@/src/native"
-ovm-srcdir	"@ABS_TOP_SRCDIR@"
-rules-dir	"@ABS_TOP_SRCDIR@/build"
-Rules.mk	"@ABS_TOP_SRCDIR@/build/Rules.mk"
-Vars.mk	"@ABS_TOP_BUILDDIR@/build/Vars.mk"
-OVMMakefile.in	"@ABS_TOP_SRCDIR@/build/OVMMakefile.in"

-include-path  "-I@ABS_TOP_SRCDIR@/src/native/common/include \
		-I@ABS_TOP_BUILDDIR@/src/native/common/include \
		-I@ABS_TOP_SRCDIR@/libffi/libffi/include \
		-I@ABS_TOP_BUILDDIR@/libffi/libffi/include"
-lib-path      "-L@ABS_TOP_BUILDDIR@/libffi/libffi/.libs \
		-L@ABS_TOP_BUILDDIR@/src/native/common"

