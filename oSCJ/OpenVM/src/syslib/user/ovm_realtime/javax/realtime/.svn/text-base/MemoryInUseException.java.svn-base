package javax.realtime;

/**
 * There has been attempt to allocate a range of physical or virtual memory 
 * that is already in use.
 *
 * @spec RTSJ 1.0.1
 */
public class MemoryInUseException extends RuntimeException {

    /**
     * A constructor for <tt>MemoryInUseException</tt>
     */
    public MemoryInUseException(){ }

    /**
     * A descriptive constructor for <tt>MemoryInUseException</tt>
     * @param description Description of the exception
     */
    public MemoryInUseException(String description) {
        super(description);
    }
}
