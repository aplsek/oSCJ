package org.ovmj.profiling;

public class EvManProfiler {
    private EvManProfiler() {}
    
    public static void resetHistograms() {
	LibraryImports.resetEvManProfileHistograms();
    }
    
    public static void disableHistograms() {
	LibraryImports.disableEvManProfileHistograms();
    }
    
    public static void startProfiling() {
	resetHistograms();
    }
    
    public static void stopProfiling() {
	disableHistograms();
    }
}

