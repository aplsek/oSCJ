package ovm.services.bytecode.verifier;

import ovm.util.OVMError;

public class VerificationError
    extends OVMError {

    public VerificationError(String message) {
	super(message);
    }
    public VerificationError(String message,
			     Throwable cause) {
	super(message, cause);
    }
    
    public VerificationError(Throwable cause) {
	super(cause);
    }
}
