# @configure_input@
# basic directory layout
# avoid the builtin @top_srcdir@, it may be relative to build
top_srcdir=@ABS_TOP_SRCDIR@
top_builddir=@ABS_TOP_BUILDDIR@
reldir=$(shell echo @abs_srcdir@ | sed 's,^$(top_srcdir),,')
prefix=@prefix@
bindir=@bindir@
infodir=@infodir@
ovm_home=@ovm_home@
VPATH=$(srcdir)

# C/C++ support
CC=@CC@
CXX=@CXX@
LD=@CC@
DEFS=@DEFS@
INCS=@INCS@
OPT=@OPT@
DEBUG=@DEBUG@
CFLAGS=@CFLAGS@
CPPFLAGS=@CPPFLAGS@
LDFLAGS=@LDFLAGS@
MULTI_OS_DIRECTORY=@MULTI_OS_DIRECTORY@
# use target instead of target_alias, because the latter may be unset
target_alias=@target@
LIBS=@LIBS@
AR=@AR@
RANLIB=@RANLIB@
NM=@NM@
OBJCOPY=@OBJCOPY@
AWK=@AWK@
INSTALL=@INSTALL@
INSTALL_DATA=@INSTALL_DATA@
MAKEINFO=@MAKEINFO@
TEXI2DVI=@TEXI2DVI@
INSTALL_INFO=@INSTALL_INFO@

CXXFLAGS=$(CFLAGS)
ALL_CFLAGS= $(CFLAGS) $(CPPFLAGS) $(DEFS) $(INCS) $(OPT) $(DEBUG)
ALL_CXXFLAGS=$(CXXFLAGS) $(DEFS) $(INCS) $(OPT) $(DEBUG)

# java/ant supprot
JAVA=@JAVA@
ANT=@ANT@
JAVADOC=@JAVADOC@
# We really shouldn't need to know about java.home.
JAVA_HOME=@JAVA_HOME@
JAVA_BOOT_PATH=@JAVA_BOOT_PATH@
JAVAC=@JAVAC@
AJC=@JAVA@ -Xmx1024M -classpath $(top_builddir)/build/aspectjtools.jar:$(JAVA_HOME)/lib/tools.jar org.aspectj.tools.ajc.Main

CONSTGEN=$(top_builddir)/src/native/constgen
MKTREE=mkdir -p
FIND_AND_EXCLUDE=/bin/sh $(top_srcdir)/build/find-and-exclude
#GEN_OVM=$(top_builddir)/bin/gen-ovm @GEN_OVM_ARGUMENTS@
GEN_OVM=$(top_builddir)/bin/gen-ovm
OVM_CONFIG=$(top_builddir)/bin/ovm-config

LINK_IMAGE=@LINK_IMAGE@
IMAGE_ARGUMENT=@IMAGE_ARGUMENT@
STATIC_LINK=@STATIC_LINK@

WEBROOT=ovmj@ovmj.org:WWW

OVM_ARCH=@OVM_ARCH@
RO=@RO@

QEMU:=@QEMU@
RTEMS_SIMULATOR:=@RTEMS_SIMULATOR@

RTEMS_BUILD:=@RTEMS_BUILD@
OVM_X86:=@OVM_X86@
OVM_SPARC:=@OVM_SPARC@
