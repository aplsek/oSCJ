<project name="compile" default="all">
  <target name="all">
    <depend srcdir=".:${srcdir}"
            destdir="."
            cache="deps"
            closure="false">
      <exclude name="${excludes}"/>
    </depend>
    <javac srcdir=".:${srcdir}"
	   destdir="."
	   debug="on"
	   bootclasspath="${bootclasspath}"
	   classpath="${classpath}"
	   excludes="${excludes}"
	   target="1.1"
           source="1.3"
           nowarn="true"/>
  </target>
</project>
