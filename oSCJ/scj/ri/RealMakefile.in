JAVAC=@JAVAC@
JAR=@JAR@

include depend.mk

all: scj.jar

clean:
	rm -rf build
	rm -f scj.jar

scj.jar: $(SRCFILES)
	rm -rf build
	mkdir build
	$(JAVAC) -nowarn -target 1.5 -source 1.5 $(SRCFILES) -cp ../../../../lib/fijicore.jar:../../../../lib/runtimej.jar:../../../../lib/fijirt.jar:../../../../lib/fivmr.jar  -d build
	$(JAR) cf scj.jar -C build .

